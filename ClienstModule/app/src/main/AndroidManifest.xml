<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.shmat.clienstmodule">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CityListActivity"></activity>
        <activity android:name=".activity.CreateCityActivity"></activity>
        <activity android:name=".activity.NationalityListActivity"></activity>
        <activity android:name=".activity.CreateNationalityActivity"></activity>
        <activity android:name=".activity.CreateDisabilityActivity"></activity>
        <activity android:name=".activity.DisabilityListActivity"></activity>
        <activity android:name=".activity.CreateFamilyActivity"></activity>
        <activity android:name=".activity.FamilyListActivity"></activity>
    </application>

</manifest>